<div class="container-fluid">
  <div id="body">
    <h1>Editing tweet</h1>

    <%= render "form", tweet: @tweet %>

    <div class="d-inline-block p-2">
      <%= link_to "Mostrar Tweet" , @tweet, :class => "btn btn-primary" %>
    </div>
    <div class="d-inline-block p-2">
      <%= link_to "Volver a los Tweets" , tweets_path, :class => "btn btn-secondary" %>
    </div>

  </div>
</div>
